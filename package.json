{
  "name": "@internachi/franklin-monorepo",
  "version": "0.0.4",
  "repository": "https://github.com/InterNACHI/franklin.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "lerna run dev --parallel",
    "build": "yarn run build-data && lerna run build --parallel",
    "build-data": "./build/index.mjs",
    "storybook": "yarn run tailwind && start-storybook -p 6006",
    "tailwind": "tailwindcss-cli build ./demo/src/css/src.css -o ./demo/src/css/tailwind.css"
  },
  "workspaces": [
    "packages/*",
    "demo"
  ],
  "devDependencies": {
    "lerna": "^3.22.1",
    "microbundle": "^0.13.0",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@tailwindcss/forms": "^0.2.1",
    "autoprefixer": "^10.1.0",
    "postcss": "^8.2.1",
    "tailwindcss": "^2.0.2",
    "tailwindcss-cli": "^0.1.2"
  }
}
